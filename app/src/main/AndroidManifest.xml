<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.sensorem.overwatch">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.DarkActionBar">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".fragment.passcodeFragment" />
        <activity android:name=".fragment.arm_alarm_Fragment" />
        <activity android:name=".fragment.disarm_alarm_fragment" />
        <activity android:name=".fragment.change_PIN_Fragment" />
        <activity android:name=".settingActivity"></activity>
    </application>

</manifest>